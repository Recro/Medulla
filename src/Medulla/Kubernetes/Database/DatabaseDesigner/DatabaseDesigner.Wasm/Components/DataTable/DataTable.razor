
@if (InTray) {
    <Entity @ref="Entity" SettingsTitle="DataTable Settings" AddToScene="@AddToScene" Diagram="@Diagram" Name="DataTable" InTray="@InTray" Image="data:image/png;base64,">
        <SettingsForm>
            <form>
                <Input WhenChanged="@QuickLoadingOnEntity" Label="Name" />
                <Comment>Changes are automatically saved.</Comment>
            </form>
        </SettingsForm>
    <NodePorts>
        @if (Node.HasPrimaryColumn)
            {
                <PortRenderer Port="Node.GetPort(PortAlignment.Right)"></PortRenderer>
            }
    </NodePorts>
        <Preview>
            @Preview
        </Preview>
    </Entity>
} else {
    <div style="position: relative;">
        <img @onclick="() => SettingsOn = !SettingsOn" class="settings-icon" style="position: absolute;
    top: -40px;
    width: 40px;
    right: 14px;
    border: thin solid lightgray;" src="https://static.vecteezy.com/system/resources/thumbnails/004/154/528/small/setting-icon-design-sign-gear-isolated-sign-free-free-vector.jpg" />

    @if (SettingsOn) {
