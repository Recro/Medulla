
@if (InTray) {
    <Entity @ref="Entity" SettingsTitle="Page Wrapper Settings" AddToScene="@AddToScene" Diagram="@Diagram" Name="Page Wrapper" InTray="@InTray" Image="https://cdn4.iconfinder.com/data/icons/documents-42/512/document_file_paper_page-31-512.png">
        <SettingsForm>
            <form>
                <Input WhenChanged="@QuickLoadingOnEntity" Label="Name" />
                <Comment>Changes are automatically saved.</Comment>
            </form>
        </SettingsForm>
    <NodePorts>
        @if (Node.HasPrimaryColumn)
            {
                <PortRenderer Port="Node.GetPort(PortAlignment.Right)"></PortRenderer>
            }
    </NodePorts>
        <Preview>
            @Preview
        </Preview>
    </Entity>
} else {
    <div style="width: 1902px;
    height: 800px;
    background-repeat: unset;
    background-size: cover;
    background-color: white;
    transition: background 300ms ease-in 200ms;
    -webkit-transition: background 200ms ease-in-out 300ms;" class="Box">
    </div>
}
